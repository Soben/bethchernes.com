<section class="module-block">
  <div class="container">
    {% include "partials/header.twig" with {
      title: item.module_header,
      subTitle: item.module_subheader
    } %}
    
    <ul class="row align-items-stretch list-unstyled flex-nowrap">
      {% for details in item.displayItems %}
      <li class="col col-4">
        <h3>{{ details.title }}</h3>
        <div class="wysiwyg-content">
          {{ details.content }}
        </div>
      </li>
      {% endfor %}
    </ul>
  </div>
</section>