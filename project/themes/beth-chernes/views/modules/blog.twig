<section class="module-block">
  <div class="container">
    {% include "partials/header.twig" with {
      title: item.module_header,
      subTitle: item.module_subheader
    } %}

    <div class="row align-items-start">
      {% for details in item.displayItems %}
      <div class="col">
        <h3>Item {{ details.title }}</h3>
      </div>
      {% endfor %}
    </div>
  </div>
</section>